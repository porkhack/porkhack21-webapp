{
  "name": "porkhack21-webapp",
  "homepage": "https://porkhack.github.io/porkhack21-webapp/",
  "version": "0.1.0",
  "scripts": {
    "start": "concurrently \"overmind-devtools\" \"export HTTPS=true&&SSL_CRT_FILE=cert.pem&&SSL_KEY_FILE=key.pem react-scripts start\"",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "prettier": {
    "semi": true,
    "arrowParens": "always"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "@emotion/core": "^11.0.0",
    "@emotion/react": "^11.1.5",
    "@emotion/styled": "^11.1.5",
    "@material-ui/core": "^4.11.3",
    "@oada/oada-cache-overmind": "^1.0.3",
    "@oada/oada-id-client": "^1.0.10",
    "@pork/trees": "^1.0.2",
    "md5": "^2.3.0",
    "moment": "^2.29.1",
    "overmind": "^27.0.0",
    "overmind-react": "^28.0.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-scripts": "^4.0.3",
    "semantic-ui-react": "^2.0.3",
    "typescript": "^4.2.3",
    "web-vitals": "^1.1.1"
  },
  "devDependencies": {
    "@types/json-pointer": "^1.0.30",
    "@types/node": "^12.19.15",
    "@types/react": "^17.0.3",
    "@types/react-dom": "^17.0.3",
    "concurrently": "^6.0.0",
    "gh-pages": "^3.1.0",
    "overmind-devtools": "^28.0.0",
    "prettier": "^2.2.1"
  }
}
